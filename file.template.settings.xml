      <template name="GraphqlConnection.java" file-name="${NAME}Connection" reformat="true" live-template-enabled="false" />
      <template name="GraphqlEdge.java" file-name="${NAME}Edge" reformat="true" live-template-enabled="false" />
    </default_templates>
  </component>
</application>